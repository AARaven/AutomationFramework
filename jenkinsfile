pipeline {

    agent any

    stages {

        stage ('version stage') {

            steps {
                echo 'mvn --version'
            }
        }

        stage ('Compilation stage') {

            steps {
                maven_3_6_0 'clean compile'
            }
        }

        stage ('Testing stage') {

            steps {
                 maven_3_6_0 'clean test'
            }
        }
    }
}